<div class="container-fluid">
  <div class="col">
    <div class="col-auto p-3 border-right">
      <nav class="nav nav-pills flex-column">
        <%_ for (const qa of qas) { -%>
          <input type="text" name="question" placeholder="Question" value="<%= qa.question -%>" class="form-control form-control-lg font-weight-bold mb-1">
          <h2><%= qa.question -%></h2>
          <p><%= qa.answer -%></p>
          <%_ } -%>
        </nav>
      </div>
    </div>
  </div>
