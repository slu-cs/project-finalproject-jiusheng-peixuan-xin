<!DOCTYPE html>
<html>
<%- include('../head'); -%>
<body>
  <%- include('../navbar', {active: 'qas'}); -%>
  <main>
    <div class="container-fluid">
      <div class="col">
        <div class="col-auto p-3 border-right">
          <nav class="nav nav-pills flex-column">
            <%_ for (const q of qa) { -%>
              <h2><%= q.question -%></h2>
              <p><%= q.answer -%></p>
              <%_ } -%>
            </nav>
          </div>

        </div>
      </div>
    </main>
  </div>
</body>
</html>
