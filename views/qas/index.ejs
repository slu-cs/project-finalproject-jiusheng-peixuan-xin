<!DOCTYPE html>
<html>
<%- include('../head'); -%>
<body>
  <%- include('../navbar', {active: 'qas'}); -%>
  <div class="container-fluid">
    <div class="col">
      <main class="p-3">
        <%_ for (const id of qaId) { -%>
          <%_ const status = (id === qa.id) ? 'active bg-dark' : 'inactive'; -%>
          <h2><%= qa.question -%></h2>
          <p><%= qa.answer -%</p>
          <%_ } -%>
        </main>
      </div>
    </div>
  </div>
</body>
</html>
